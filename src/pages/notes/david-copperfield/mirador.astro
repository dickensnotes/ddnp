---
import MiradorFullScreenLayout from "../../../layouts/mirador-full-screen-layout.astro";
import Mirador from "../../../components/Mirador";

const manifest =
  "https://dickensnotes.github.io/dickens-annotations/img/derivatives/iiif/davidcopperfieldtranscription/manifest.json";
---

<MiradorFullScreenLayout
  title="David Copperfield Mirador viewer"
  description="Transcriptions of Charles Dickens's Working Notes for David Copperfield viewed in Mirador"
>
  <Mirador client:only="react" loadedManifest={manifest} />
</MiradorFullScreenLayout>
